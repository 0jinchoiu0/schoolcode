#包括 <stdio.h>

void output(int *arr,int size){
 printf ("A [%d]={", size); 
 for (int i = 0; i < size; i++) { 
 printf("%d", *(arr + i)); 
 if (i < size - 1) { 
 printf (","); 
         } 
     } 
 printf ("}\n"); 
}

int main () {
 int 大小； 
    
 while (scanf ("%d", &size) & size != 0) { 
 int arr [size]; 
 for (int i = 0; i < size; i++) { 
 scanf ("%d", &arr [i]); 
         } 
 output (arr, size); 
     } 
    
 返回 0; 
}
